<!-- contact -->
<section class="section-with-bg" id="contact">
  <div class="container mt-5">
    <div class="eleven">
      <h1 class="center mt-1 mb-5 m-auto  text-capitaliz big_font position-relative">Contact us</h1>
    </div>
    <div class="row">
      <div class="col-md-6 col-12">
        <div class="contact-map h-100">
          <iframe
            src="https://www.google.com/maps/embed?pb=!1m17!1m12!1m3!1d3595.582849285996!2d81.78725687525937!3d21.12955408054513!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m2!1m1!2zMjHCsDA3JzQ2LjQiTiA4McKwNDcnMjMuNCJF!5e1!3m2!1sen!2sin!4v1736053844716!5m2!1sen!2sin"
            width="100%" height="400" class="h-100" style="border:0;" allowfullscreen="" loading="lazy"
            referrerpolicy="no-referrer-when-downgrade"></iframe>
        </div>
      </div>

      <!-- Contact form -->
      <div class="col-md-6 col-12">



        <form id="ajax-form" action="/contact" method="post" class="form-main">
          <!-- <input type="hidden" name="redirectTo" value="index"> -->
          <div class="form-group">
            <label for="name">Name</label>
            <input class="form-control" id="name" name="name" type="text" placeholder="Enter your name" required>
            <div class="error" id="err-name" style="display: none;">Please enter name</div>
          </div>
          <div class="form-group">
            <label for="mobile">Mobile</label>
            <input class="form-control" id="mobile" name="mobile" type="number" placeholder="Enter your mobile number">
            <div class="error" id="err-mobile" style="display: none;">Mobile is not valid</div>
          </div>
          <div class="form-group">
            <label for="email">Email</label>
            <input class="form-control" id="email" name="email" type="text" placeholder="Enter your email" required>
            <div class="error" id="err-emailvld" style="display: none;">E-mail is not valid</div>
          </div>
          <div class="form-group">
            <label for="message">Type your query here</label>
            <textarea class="form-control" id="message" name="message" rows="4" required></textarea>
          </div>
          <div id="error-message" class="error" style="display: none; color: red;"></div>
          <div class="d-flex justify-content-end mt-4">
            <button type="submit" class="btn rounded-0 p-3 px-3 btnHover" id="send">Submit</button>
          </div>
        </form>
        <div id="thanks-message" style="display: none;">
          <h4>Thank you for your message!</h4>
          <p>We will get back to you soon.</p>
        </div>

      </div>

    </div>

  </div>
</section>
