<!-- hotels -->
<section id="hotels" class="">

  <div class="container" data-aos="fade-up">
    <div class="eleven">
      <h1 class="center mt-1 mb-5 m-auto  text-capitaliz big_font position-relative">Our Destinaitons</h1>
    </div>
    <!-- <p>Her are some nearby hotels</p> -->

    <div class="row mb-5" data-aos="fade-up" data-aos-delay="100">
      <% hotels.forEach((hotel,index)=>{%>
        <% if(hotel.status==2){ %>
          <div class="col-lg-4 col-md-6">
            <div class="hotel mb-0 position-relative" style="height: 20rem;">
              <img src="//<%= hotel.main_image %>" alt="<%= hotel.name %>" class="border-0 rounded-0"
                style="height: 100%;width: 100%;object-fit: cover;">
              <div style="background: white;    bottom: -2px;" class=" position-absolute">
                <h3>Hotels in <%= hotel.city.charAt(0).toUpperCase() + hotel.city.slice(1).toLowerCase()%>
                </h3>
                <p class="text-end mb-0 fw-bold"><a
                    href="/hotels/<%= hotel.name ? hotel.name.toLowerCase().replace(/\s+/g, '-') : '' %>"
                    class="inter"> view Hotel</a></p>
              </div>

            </div>
          </div>
          <% }%>
            <% }) %>
    </div>
    <div class="row" data-aos="fade-up" data-aos-delay="100">

      <!-- <div class="col-12">
        <h2 style="text-align: center;">Coming soon</h2>
      </div> -->
      <% hotels.forEach((hotel,index)=>{%>
        <% if(hotel.status==1){ %>
          <div class="col-lg-4 col-md-6 mb-5">
            <div class="hotel mb-0 position-relative" style="height: 20rem;">
              <img src="//<%= hotel.main_image %>" alt="<%= hotel.name %>" class="border-0 rounded-0"
                style="height: 100%;width: 100%;object-fit: cover;">
              <div style="background: white;    bottom: -2px;" class=" position-absolute">
                <h3>Hotels in <%= hotel.city.charAt(0).toUpperCase() + hotel.city.slice(1).toLowerCase()%>
                </h3>
                <p class="text-end mb-0 fw-bold"><a
                    href="/hotels/<%= hotel.name ? hotel.name.toLowerCase().replace(/\s+/g, '-') : '' %>"
                    class="inter"> view Hotel</a></p>
                <p class="text-end mb-0 fw-bold"><a href="#" class="inter"> Coming Soon</a></p>
              </div>

            </div>
          </div>
          <% }%>
            <% }) %>
    </div>
  </div>

</section>
