<script type="text/javascript">
  document.addEventListener('DOMContentLoaded', () => {
    const cityTabs = document.querySelectorAll('.city-tab');
    const hotelLists = document.querySelectorAll('.hotel-list');

    cityTabs.forEach(tab => {
      tab.addEventListener('click', () => {
        // Remove active class from all city tabs
        cityTabs.forEach(t => t.classList.remove('active'));

        // Add active class to the clicked city
        tab.classList.add('active');

        // Hide all hotel lists
        hotelLists.forEach(list => list.classList.add('d-none'));

        // Show the hotel list for the selected city
        const city = tab.getAttribute('data-city');
        document.querySelector(`.hotel-list[data-city="${city}"]`).classList.remove('d-none');
      });
    });
  });
  document.getElementById('redirectUrl').value = window.location.href;

  function googleTranslateElementInit() {
    new google.translate.TranslateElement({ pageLanguage: 'en', layout: google.translate.TranslateElement.InlineLayout.SIMPLE }, 'google_translate_element');
  }
</script>
