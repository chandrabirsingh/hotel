      .btnHover {
        scale: 1;
        border: 1px solid #005f96;
        transition: all .5s ease;
      }

      .btnHover:hover {
        background: white;
        color: black;
        scale: .9;
        border: 1px solid black;

      }


      .imgHov {
        box-shadow: 12px 12px 1px 0 rgb(56, 56, 56), -12px -12px 1px 0 rgb(132, 207, 250);
        transition: box-shadow 0.4s ease-in-out;
      }

      .imgHov:hover {
        animation: shadowAnim 0.6s ease-in-out forwards;
      }

      @keyframes shadowAnim {
        0% {
          box-shadow: 12px 12px 1px 0 rgb(56, 56, 56), -12px -12px 1px 0 rgb(132, 207, 250);
        }

        50% {
          box-shadow: 0px 0px 1px 0 rgb(132, 207, 250), 0px 0px 1px 0 rgb(56, 56, 56);
        }

        100% {
          box-shadow: 12px 12px 1px 0 rgb(132, 207, 250), -12px -12px 1px 0 rgb(56, 56, 56);
        }
      }


      .services-right .icon-box {
        flex-direction: row-reverse;
      }

      .grid-container {
        display: grid;
        grid-template-columns: 1fr 1fr 1fr;
        gap: 30px;
        align-items: center;
        padding: 20px;
      }

      .services-left,
      .services-right {
        display: grid;
        grid-template-columns: 1fr;
        gap: 30px;
      }

      .image-center {
        display: flex;
        justify-content: center;
        align-items: center;
        height: 100%;
        border-radius: 100%;
        background: #70c9a9;
        overflow: hidden;
      }

      .image-center img {
        width: 100%;
        height: 100%;
        border-radius: 10px;
        object-fit: cover;
        transform: translate(0px, 10px);

      }

      .icon-box {
        text-align: center;
        padding: 10px !important;
        background: none !important;
        border: none;
        border-radius: none;
        box-shadow: none !important;
        transition: box-shadow 0.3s ease;
        transform: translateX(var(--angle));
        gap: 5px;

      }

      .icon-box:hover {
        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
      }

      .icon-box i {
        font-size: 2rem;
        padding: 0px !important;
      }

      .icon-box h3 {
        font-size: 1.2rem;
        margin-top: 10px;
        font-weight: 800;
        ;
        font-family: "Inter", sans-serif !important;
      }

      .testimonial .pic img {
        width: 100%;
        height: 100%;
        object-fit: cover;
      }

      .testimonial .pic {
        display: inline-block;
        position: static;
        min-width: 80px;
        height: 80px;
        border-radius: 50%;
        overflow: hidden;
      }

      .image-center.sm_sc {
        display: none;
      }

      @media (max-width: 1024px) {
        .grid-container {
          grid-template-columns: 1fr 1fr;
          /* 2 columns */
        }

        .image-center.sm_sc {
          display: flex;
        }

        .image-center.sm_sc img {
          transform: translate(0px, 10px) rotateX(153deg) rotateZ(180deg);
        }
      }

      @media (max-width: 576px) {
        .grid-container {
          grid-template-columns: 1fr;
          /* 2 columns */
        }

        .image-center {
          display: none;
        }

        .image-center.sm_sc {
          display: none;
        }

        .icon-box {
          transform: none;
        }

        .services-right .icon-box {
          flex-direction: row;
        }

        #hero .hero-container {
          top: unset;
          transform: unset;
        }
      }
